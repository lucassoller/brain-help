<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B9E5FF"
    tools:context=".activities.TelaHomeActivity">

    <ImageView
        android:id="@+id/iv_contatos"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/contatos"
        tools:srcCompat="@drawable/contatos"
        />

    <ImageView
        android:id="@+id/iv_enderecos"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:padding="5dp"
        android:background="@drawable/image_corner_white"
        android:src="@drawable/house"
        tools:srcCompat="@drawable/house" />

    <ImageView
        android:id="@+id/iv_medicamentos"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/medicine"
        tools:srcCompat="@drawable/medicine" />

    <ImageView
        android:id="@+id/iv_musicas"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/music"
        tools:srcCompat="@drawable/music" />

    <ImageView
        android:id="@+id/iv_fotos"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/album"
        tools:srcCompat="@drawable/album" />

    <ImageView
        android:id="@+id/iv_jogos"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/puzzle"
        tools:srcCompat="@drawable/puzzle" />

    <ImageView
        android:id="@+id/iv_tarefas"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/tarefas"
        tools:srcCompat="@drawable/tarefas" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="27dp"
        android:text="Família e Amigos"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="78dp"
        android:layout_marginRight="78dp"
        android:text="Jogos"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_contatos"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="27dp"
        android:text="Chatbot"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_jogos"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="78dp"
        android:layout_marginRight="78dp"
        android:text="Tarefas"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_chatbot"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="27dp"
        android:text="Músicas"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_tarefas"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="39dp"
        android:layout_marginRight="39dp"
        android:text="Álbum de fotos"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_musicas"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="27dp"
        android:text="Medicamentos"
        android:textColor="#000000"
        android:textSize="10pt" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_fotos"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:text="Endereços"
        android:textColor="#000000"
        android:textSize="10pt" />

    <ImageView
        android:id="@+id/iv_chatbot"
        android:layout_width="177dp"
        android:layout_height="115dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/image_corner_white"
        android:padding="5dp"
        android:src="@drawable/chatbot"
        tools:srcCompat="@drawable/chatbot" />
</RelativeLayout>