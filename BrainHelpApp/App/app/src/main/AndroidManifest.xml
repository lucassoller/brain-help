<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_brain_help"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_brain_help_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.TelaHomeActivity"></activity>
        <activity android:name=".activities.TelaCadastroActivity" />
        <activity android:name=".activities.TelaLoginActivity" />
        <activity android:name=".activities.TelaLogin" />
        <activity android:name=".activities.TelaInicialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>